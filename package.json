{
  "name": "ultima-orb",
  "version": "0.1.0",
  "description": "Modular AI features for Obsidian",
  "private": false,
  "main": "main.js",
  "scripts": {
    "prepare": "husky",
    "build": "tsc -p tsconfig.json",
    "type-check": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint .",
    "test": "vitest run"
  },
  "lint-staged": {
    "*.{ts,tsx,js,css,md}": [
      "prettier --write"
    ],
    "*.{ts,tsx,js}": [
      "eslint --fix"
    ]
  },
  "keywords": [
    "obsidian-plugin",
    "ai",
    "rag",
    "agent",
    "hybrid-ai",
    "knowledge-management"
  ],
  "author": "billlzzz10",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitest/ui": "^3.2.4",
    "builtin-modules": "^3.2.0",
    "dotenv": "^16.3.1",
    "esbuild": "0.25.0",
    "eslint": "^8.57.0",
    "obsidian": "latest",
    "typescript": "^5.4.0",
    "vitest": "^3.2.4",
    "husky": "^9.0.0",
    "lint-staged": "^15.2.0"
  },
  "dependencies": {
    "@notionhq/client": "^2.2.13",
    "eventemitter3": "^5.0.1",
    "tslib": "^2.8.1"
